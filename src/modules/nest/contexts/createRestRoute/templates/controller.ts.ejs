import { Body, Controller, Get, HttpStatus} from '@nestjs/common';
import { ApiResponse, ApiTags } from '@nestjs/swagger';

import {<%= routeName %>RequestDTO} from "./dtos/request.dto";
import { <%= routeName %>ResponseDTO } from "./dtos/response.dto";
import { <%= routeName %>Service } from "./<%= routeName %>.service";

@ApiTags("Example")
@Controller()
export class <%= routeName %>Controller{
    constructor(private <%= routeName %>Service: <%= routeName %>Service){}

  @Get('/example')
  @ApiResponse({
    status: HttpStatus.OK,
    description: 'OK',
    type: <%= routeName %>ResponseDTO,
  })
  async handle(@Body() body: <%= routeName %>RequestDTO):Promise<any>{
    return body;
  }
}