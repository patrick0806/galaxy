import { Body, Controller, Get, HttpStatus} from '@nestjs/common';
import { ApiResponse, ApiTags } from '@nestjs/swagger';

import {<%= className %>RequestDTO} from "./dtos/request.dto";
import { <%= className %>ResponseDTO } from "./dtos/response.dto";
import { <%= className %>Service } from "./<%= fileName %>.service";

@ApiTags("Example")
@Controller()
export class <%= className %>Controller{
    constructor(private <%= className %>Service: <%= className %>Service){}

  @Get('/example')
  @ApiResponse({
    status: HttpStatus.OK,
    description: 'OK',
    type: <%= className %>ResponseDTO,
  })
  async handle(@Body() body: <%= className %>RequestDTO):Promise<any>{
    return body;
  }
}